<Window x:Class="SampleApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SampleApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="170" Width="400">
    <Grid>
        <StackPanel>
            <Button Click="PathClicked" Margin="4" FontSize="20">Path Point Example</Button>
            <Button Click="DotClicked" Margin="4" FontSize="20">Dot Point Example</Button>
            <Button Click="FileClicked" Margin="4" FontSize="20">File Example</Button>
        </StackPanel>
        
    </Grid>
</Window>
